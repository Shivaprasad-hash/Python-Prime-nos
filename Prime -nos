# To print Prime Numbers from 1 to 100

start=2
stop=14
count=[]
for i in range(start,stop):
    for j in range(2,i):
        if(i%j == 0):
            break
    else:
        print(i)
        count.append(i)

print("No. of Prime numbers:",len(count))

# To say given number is prime or not
n = int(input())

if(n>1):
    for i in range(2,n):
        if(n%i == 0):
            print("Not Prime")
            break
    else:
        print("Prime")
else:
    print("Not Prime")
